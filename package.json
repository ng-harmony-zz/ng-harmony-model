{
  "name": "ng-harmony-model",
  "description": "In-Memory-DB, Storage, Adapters, DB, $scope",
  "version": "0.0.7",
  "homepage": "https://github.com/ng-harmony/ng-harmony-model",
  "author": {
    "name": "joehannes",
    "email": "johannes.neugschwentner@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ng-harmony/ng-harmony-model.git"
  },
  "bugs": {
    "url": "https://github.com/ng-harmony/ng-harmony-model/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ng-harmony/ng-harmony-model/blob/master/LICENSE"
    }
  ],
  "main": "build/index.js",
  "engines": {
    "node": ">= 0.10.36",
    "npm": ">=1.4.28"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "literate-programming src/ng_harmony.md && npm run modules",
    "transpile_amd": "babel --modules amd -e 1 -s -o ./build/modules/amd_module.js ./build/index.js",
    "transpile_common": "babel --modules common -e 1 -s -o ./build/modules/common_module.js ./build/index.js",
    "transpile_system": "babel --modules system -e 1 -s -o ./build/modules/system_module.js ./build/index.js",
    "transpile_umd": "babel --modules umd -e 1 -s -o ./build/modules/umd_module.js ./build/index.js",
    "modules": "npm run transpile_amd && npm run transpile_common && npm run transpile_system && npm run transpile_umd"
  },
  "dependencies": {
    "ng-harmony-core": "^0.1.2",
    "ng-harmony-decorator": "^0.3.13",
    "ng-harmony-log": "^0.1.7"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2017": "^6.14.0",
    "babel-preset-stage-3": "^6.5.0",
    "flow-bin": "^0.31.1",
    "literate-programming": "^0.8.4"
  },
  "keywords": [],
  "jspm": {
    "dependencies": {
      "whatwg-fetch": "npm:whatwg-fetch@^1.0.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
